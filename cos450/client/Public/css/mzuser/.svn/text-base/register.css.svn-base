.register-form{
	border:1px solid #abcdef;
	width:420px;
	height:460px;
	position: absolute;
	left:460px;
	top:50px;
	background-attachment: scroll;
	background: #F8F8F8;
	border: 1px solid #E0E0E0;
}
.register-form h3{
	text-align: left;
	padding:0 0 0 10px;
	border-bottom: 1px solid #000;
}
.tips-code{
	margin:0 0 0 65px;
	font-size:13px;
}
body {
	background-attachment: scroll;
	background: #fff;
}
h2{
	color: #494949;
	display: block;
	font-size: 25px;
	font-weight: bold;
	line-height: 1.1;
	margin: 0;
	padding: 0 30px 15px;
	word-wrap: break-word;
}
.login_content{
	margin:100px auto 0;
	width:810px;
	height:300px;
}

.left_img{
	width:400px;
	height:300px;
	float:left;
}
.right_form{
	float:right;

	width:400px;
	height:300px;
}
.item{
    clear: both;
    margin: 15px 0;
}
.item a{
    text-decoration: none;
}
.item_tw{
    clear: both;
    margin: 15px 0 0 20px;
}
.item_th{
    clear: both;
    margin: 15px 0 0 20px;
}
label{
	display: inline-block;
    float: left;
    font-size: 14px;
    line-height: 30px;
    margin-right: 15px;
    text-align: right;
    vertical-align: baseline;
    width: 60px;
}
 .basic-input{
	border: 1px solid #c9c9c9;
    border-radius: 3px;
    font-size: 14px;
    height: 29px;
    padding: 5px;
    vertical-align: middle;
    width: 200px;
}
.remember {
    color: #666;
    cursor: pointer;
    display: inline;
    float: none;
    font-size: 12px;
    margin: 0;
    text-align: left;
    width: auto;
}
.btn-submit {
    background: none repeat scroll 0 0 #60BFD1;
    border: 1px solid #60BFD1;
    border-radius: 3px;
    color: #ffffff;
    cursor: pointer;
    font-size: 14px;
    font-weight: bold;
    padding: 6px 26px;
}
.enabled {
    background: none repeat scroll 0 0 #3fa156;
    border: 1px solid #528641;
    color: #ffffff;
}
.item img{
	padding: 0 0 0 0;
	position: absolute;
	margin: 0 0 0 5px;
}
.register{
	color: #666;
	display: inline;
	float: none;
	font-size: 12px;
	margin: 0;
	text-align: left;
	width: auto;
}
		a { color: #336699 }
		em { font-style: normal }
		form { position: relative; left: 0; top: 0 }
